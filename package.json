{
  "name": "unit-testing-example",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": " mocha test/**/*.spec.js --exit --reporter mochawesome",
    "coverage": "c8 npm test",
    "lint": "eslint ./ --fix"
  },
  "nyc": {
    "exclude": [
      "dist",
      "node_modules",
      "coverage",
      "mochawesome-report"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "author": "LAUFY",
  "license": "ISC",
  "description": "unit testing example",
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "globals": "^15.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0"
  },
  "dependencies": {
    "c8": "^9.1.0",
    "chai": "^5.1.0",
    "husky": "^9.0.11"
  }
}
